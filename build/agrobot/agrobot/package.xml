<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>agrobot</name>
  <version>0.0.0</version>
  <description>
    Package containing all relevant agrobot packages for Reinforcement learning using the digital twin of the agrobot
  </description>

  <maintainer email="marnix.teunissen@gmail.com">Marnix Teunissen</maintainer>
  <license>TODO: License declaration</license>

  <buildtool_depend>ament_cmake</buildtool_depend>
  <buildtool_depend>ament_cmake_python</buildtool_depend>

  <exec_depend>agrobot_description</exec_depend>
  <exec_depend>agrobot_simulation</exec_depend>
  <exec_depend>agrobot_control</exec_depend>
  <exec_depend>agrobot_rl</exec_depend>
  <exec_depend>custom_interfaces</exec_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
